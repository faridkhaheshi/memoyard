{
  "name": "nextjs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migration-status-dev": "hasura migrate status --envfile ../.env.development.local --project database --database-name default",
    "migration-status-prod": "hasura migrate status --envfile ../.env.production.local --project database --database-name default",
    "db-migration-init": "hasura migrate create 'start' --from-server --envfile ../.env.development.local --project database --database-name default",
    "hasura-console-dev": "hasura console --envfile ../.env.development.local --project database",
    "hasura-console-prod": "hasura console --envfile ../.env.production.local --project database",
    "hasura-fetch-metadata-dev": "hasura metadata export --envfile ../.env.development.local --project database",
    "db-update-metadata-prod": "hasura metadata apply --envfile ../.env.production.local --project database",
    "db-migrate-prod": "hasura migrate apply --envfile ../.env.production.local --project database --all-databases",
    "db-reload-metadata-prod": "hasura metadata reload --envfile ../.env.production.local --project database",
    "seed-dev": "hasura seed apply --envfile ../.env.development.local --project database --database-name default",
    "seed-prod": "hasura seed apply --envfile ../.env.production.local --project database --database-name default"
  },
  "dependencies": {
    "@apollo/client": "3.5.5",
    "@aws-sdk/client-rds": "3.39.0",
    "@emotion/cache": "11.6.0",
    "@emotion/react": "11.7.0",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.6.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@mui/icons-material": "5.0.0-rc.1",
    "@mui/lab": "5.0.0-alpha.58",
    "@mui/material": "5.0.0-rc.1",
    "@mui/x-data-grid": "5.0.1",
    "aws-cloudfront-sign": "1.1.0",
    "aws-sdk": "2.1018.0",
    "classnames": "2.2.6",
    "clsx": "1.1.1",
    "data-api-client": "1.2.0",
    "graphql": "0.11.7",
    "js-cookie": "3.0.1",
    "jsonwebtoken": "8.5.1",
    "next": "12.0.1",
    "next-seo": "4.28.1",
    "nprogress": "0.2.0",
    "ra-data-hasura": "0.2.0",
    "react": "17.0.2",
    "react-admin": "3.19.1",
    "react-dom": "17.0.2",
    "react-json-view": "1.21.3",
    "react-masonry-css": "1.0.16",
    "restify-errors": "8.0.2",
    "sass": "1.43.4",
    "swr": "1.0.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "eslint-config-prettier": "8.3.0"
  }
}
